class Material extends Entity{

    final private double level1;
    final private double level2;
    final private double level3;

    Material(String name, String description, int id, double level1, double level2, double level3){
        super(name, description, id);
        this.level1=level1;
        this.level2=level2;
        this.level3=level3;
    }

    @Override
    public String getDetails(){
        return "EntityType: Materil One person is entitled to have" + level1 + "2 to 4 persons are entitled to have "+ level2 + "5+ persons are entitled to have " + level3;
    }

    public double getLevel1(){
        return level1;
    }

    public double getLevel2(){
        return level2;
    }

    public double getLevel3(){
        return level3;
    }
}
